preprocess: $(sources)
	$(CC) $(CFLAGS) /P $(SRC_DIR)\Input.c /Fi:$(SRC_DIR)\Input.i
	$(CC) $(CFLAGS) /P $(SRC_DIR)\Algorithm.c /Fi:$(SRC_DIR)\Algorithm.i
	$(CC) $(CFLAGS) /P $(SRC_DIR)\Procedure.c /Fi:$(SRC_DIR)\BFS\Procedure.i
	$(CC) $(CFLAGS) /P $(SRC_DIR)\PA\Input.c /Fi:$(SRC_DIR)\PA\Input.i
