compile: preprocess $(objects)
	Get-ChildItem -Path .\src\ -Directory -recurse | Out-File FileTree.txt
	Get-Content -Path .\FileTree | New-Item -Path .\obj -ItemType "Directory"
# 	New-Item -Path . -ItemType "Directory" -Name obj/
# 	New-Item -Path ./obj -Name 
	$(CC) $(CFLAGS) /c $(SRC_DIR)\Input.i /Fo:$(OBJ_DIR)\Input.o
	$(CC) $(CFLAGS) /c $(SRC_DIR)\Algorithm.i /Fo:$(OBJ_DIR)\Algorithm.o
	$(CC) $(CFLAGS) /c $(SRC_DIR)\Procedure.i /Fo:$(OBJ_DIR)\BFS\Procedure.o
	$(CC) $(CFLAGS) /c $(SRC_DIR)\PA\Input.i /Fo:$(OBJ_DIR)\PA\Input.o
	$(CC) $(CFLAGS) /c $(SRC_DIR)\Output.i /Fo:$(OBJ_DIR)\PA\Output.o
	$(CC) $(CFLAGS) /c $(SRC_DIR)\BFS\Record.i /Fo:$(OBJ_DIR)\BFS\Record.o
	$(CC) $(CFLAGS) /c $(SRC_DIR)\Output.i /Fo:$(OBJ_DIR)\Output.o 
	$(CC) $(CFLAGS) /c $(SRC_DIR)\PA\Data.i /Fo:$(OBJ_DIR)\PA\Data.o
	$(CC) $(CFLAGS) /c $(SRC_DIR)\PA\Destination.i /Fo:$(OBJ_DIR)\PA\Destination.o
	$(CC) $(CFLAGS) /c $(SRC_DIR)\PA\Tree.i /Fo:$(OBJ_DIR)\PA\Tree.o
	$(CC) $(CFLAGS) /c $(SRC_DIR)\PA\List.i /Fo:$(OBJ_DIR)\PA\List.o
